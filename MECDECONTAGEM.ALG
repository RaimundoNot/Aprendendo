Algoritmo "MecDeContagem"

Var
contador, salto, val: inteiro


Inicio
      contador <- 0
      Escreva ("Digite o valor da contagem: ")
      Leia (val)
      Escreva ("Qual o valor do salto? ")
      Leia (salto)
      Enquanto (contador <= val) faca
         Escreval (contador)
         contador <- contador + salto
      Fimenquanto
         Escreval ("Terminei de contar")


Fimalgoritmo